[manager]
docker-swarm-manager ansible_host=REPLACETHIS
[manager:vars]
ansible_user=root
ansible_private_key_file=~/.ssh/id_rsa
ansible_python_interpreter=/usr/bin/python3

[worker]
docker-swarm-worker-0 ansible_host=REPLACETHIS
docker-swarm-worker-1 ansible_host=REPLACETHIS
[worker:vars]
ansible_user=root
ansible_private_key_file=~/.ssh/id_rsa
ansible_python_interpreter=/usr/bin/python3
